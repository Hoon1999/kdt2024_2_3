<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.TilePane?>

<TilePane xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx"
 alignment="CENTER" hgap="5.0" prefWidth="300.0" vgap="5.0" >
   <children>
      <Button fx:id="btn1" mnemonicParsing="false" prefHeight="50.0" prefWidth="80.0" text="1" />
      <Button fx:id="btn2" mnemonicParsing="false" prefHeight="50.0" prefWidth="80.0" text="2" />
      <Button fx:id="btn3" mnemonicParsing="false" prefHeight="50.0" prefWidth="80.0" text="3" />
      <Button fx:id="btn4" mnemonicParsing="false" prefHeight="50.0" prefWidth="80.0" text="4" />
      <Button fx:id="btn5" mnemonicParsing="false" prefHeight="50.0" prefWidth="80.0" text="5" />
      <Button fx:id="btn6" mnemonicParsing="false" prefHeight="50.0" prefWidth="80.0" text="6" />
      <Button fx:id="btn7" mnemonicParsing="false" prefHeight="50.0" prefWidth="80.0" text="7" />
      <Button fx:id="btn8" mnemonicParsing="false" prefHeight="50.0" prefWidth="80.0" text="8" />
      <Button fx:id="btn9" mnemonicParsing="false" prefHeight="50.0" prefWidth="80.0" text="9" />
      <Button fx:id="btnEraseOne" mnemonicParsing="false" prefHeight="50.0" prefWidth="80.0" text="하나 지우기" />
      <Button fx:id="btn0" mnemonicParsing="false" prefHeight="50.0" prefWidth="80.0" text="0" />
      <Button fx:id="btnEraseAll" mnemonicParsing="false" prefHeight="50.0" prefWidth="80.0" text="전체 지우기" />
   </children>
   <padding>
      <Insets bottom="10.0" top="20.0" />
   </padding>
</TilePane>
