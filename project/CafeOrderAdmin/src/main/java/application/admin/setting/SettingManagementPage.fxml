<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>

<BorderPane fx:id="bp"
 xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml/1"
 maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
 prefHeight="400.0" prefWidth="600.0" >
   <center>
	<VBox fx:id="vbDefault" alignment="CENTER" spacing="20.0">
	   <children>
	      <Button fx:id="btnChangePassword" mnemonicParsing="false" prefWidth="200.0" text="비밀번호 변경">
	         <padding>
	            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
	         </padding>
	      </Button>
	      <Button fx:id="btnLogout" mnemonicParsing="false" prefWidth="200.0" text="로그아웃">
	         <padding>
	            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
	         </padding>
	      </Button>
	      <Button fx:id="btnClose" mnemonicParsing="false" prefWidth="200.0" text="프로그램 종료(마감)">
	         <padding>
	            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
	         </padding>
	      </Button>
	   </children>
	   <padding>
	      <Insets bottom="100.0" left="100.0" right="100.0" top="100.0" />
	   </padding>
	</VBox>
   </center>
</BorderPane>
